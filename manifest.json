{
  "manifest_version": 2,
  "name": "KBBI Search",
  "version": "1.0",

  "description": "KBBI Search",

  "icons": {
    "48": "icons/border-48.png"
  },

  "browser_action": {
    "default_title": "KBBI Search",
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["borderify.js"]
    }
  ]
}
